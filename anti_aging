    def handle_fasting(self, query: str, user, context: Dict) -> Dict:
        """–û–±—Ä–æ–±–∫–∞ –∑–∞–ø–∏—Ç—ñ–≤ –ø—Ä–æ –≥–æ–ª–æ–¥—É–≤–∞–Ω–Ω—è"""
        
        fasting_data = self.data.get('fasting_protocols', {})
        
        # –ü–µ—Ä–µ–≤–∞–≥–∏ –≥–æ–ª–æ–¥—É–≤–∞–Ω–Ω—è
        benefits = fasting_data.get('benefits', {}).get('mechanisms', [])
        
        # –ü—Ä–æ—Ç–æ–∫–æ–ª–∏ IF
        if_protocols = []
        for protocol in fasting_data.get('intermittent_fasting', {}).get('protocols', []):
            if_protocols.append({
                'name': protocol['name'],
                'description': protocol['description'],
                'example': protocol.get('example', ''),
                'difficulty': protocol['difficulty'],
                'benefits': protocol['benefits'],
                'tips': protocol.get('tips', [])
            })
        
        # –¢—Ä–∏–≤–∞–ª–µ –≥–æ–ª–æ–¥—É–≤–∞–Ω–Ω—è
        extended_fasting = fasting_data.get('extended_fasting', {}).get('protocols', [])
        
        # –ü—ñ–¥—Ç—Ä–∏–º–∫–∞ –ø—ñ–¥ —á–∞—Å –≥–æ–ª–æ–¥—É–≤–∞–Ω–Ω—è
        fasting_support = fasting_data.get('fasting_support', {})
        
        # –ö–æ–º—É –∫–æ—Ä–∏—Å–Ω–æ
        who_benefits = fasting_data.get('who_benefits', {})
        
        return {
            'type': 'fasting',
            'title': '‚è∞ –ü—Ä–æ—Ç–æ–∫–æ–ª–∏ –≥–æ–ª–æ–¥—É–≤–∞–Ω–Ω—è',
            'description': '–Ü–Ω—Ç–µ—Ä–≤–∞–ª—å–Ω–µ —Ç–∞ —Ç—Ä–∏–≤–∞–ª–µ –≥–æ–ª–æ–¥—É–≤–∞–Ω–Ω—è –¥–ª—è –¥–æ–≤–≥–æ–ª—ñ—Ç—Ç—è',
            'benefits': benefits,
            'intermittent_fasting': if_protocols,
            'extended_fasting': extended_fasting,
            'fasting_support': fasting_support,
            'who_benefits': who_benefits.get('indications', []),
            'contraindications': who_benefits.get('contraindications', []),
            'getting_started': [
                'üöÄ –ü–æ—á–Ω—ñ—Ç—å –∑ 16:8 –ø—Ä–æ—Ç–æ–∫–æ–ª—É',
