        # –ü—Ä–∞–∫—Ç–∏–∫–∏ –¥–ª—è –º–æ–∑–∫—É
        brain_practices = cognitive_data.get('brain_practices', {}).get('activities', [])
        
        return {
            'type': 'cognitive',
            'title': 'üß† –ö–æ–≥–Ω—ñ—Ç–∏–≤–Ω–µ –∑–¥–æ—Ä–æ–≤\'—è —Ç–∞ –º–æ–∑–æ–∫',
            'description': '–û–ø—Ç–∏–º—ñ–∑–∞—Ü—ñ—è —Ä–æ–∑—É–º–æ–≤–æ—ó –¥—ñ—è–ª—å–Ω–æ—Å—Ç—ñ —Ç–∞ –Ω–µ–π—Ä–æ–ø–ª–∞—Å—Ç–∏—á–Ω–æ—Å—Ç—ñ',
            'morning_focus_stack': morning_stack,
            'neurogenesis_memory': memory_stack,
            'cognitive_protocols': protocols,
            'brain_practices': brain_practices,
            'key_benefits': [
                'üéØ –ü–æ–∫—Ä–∞—â–µ–Ω–Ω—è —Ñ–æ–∫—É—Å—É —Ç–∞ –∫–æ–Ω—Ü–µ–Ω—Ç—Ä–∞—Ü—ñ—ó',
