# ===========================================
# 3. ROUTER UPDATES
# ===========================================

# assistant/base.py (–û–Ω–æ–≤–ª–µ–Ω–Ω—è AssistantRouter)
class AssistantRouter:
    def __init__(self):
        self.handlers = {
            'supplements': SupplementsHandler(),
            'steroids': SteroidsHandler(),
            'blood': BloodAnalysisHandler(),
            'lifestyle': LifestyleHandler(),
            'womens_health': WomensHealthHandler(),
            'anti_aging': AntiAgingHandler()  # –î–û–î–ê–ù–û
        }
        self.matcher = QueryMatcher()
    
    def route_query(self, query, user, context):
        """–ú–∞—Ä—à—Ä—É—Ç–∏–∑–∞—Ü—ñ—è –∑–∞–ø–∏—Ç—É –¥–æ –≤—ñ–¥–ø–æ–≤—ñ–¥–Ω–æ–≥–æ –æ–±—Ä–æ–±–Ω–∏–∫–∞"""
        
        # Determine query type
        query_type = self.matcher.classify_query(query)
        
        # Check user access
        if not user.has_access(query_type):
            return {
                'error': 'Access denied',
                'upgrade_required': True,
                'message': f'–ü–æ—Ç—Ä—ñ–±–Ω–∞ –ø—ñ–¥–ø–∏—Å–∫–∞ –¥–ª—è –¥–æ—Å—Ç—É–ø—É –¥–æ –º–æ–¥—É–ª—è {query_type}',
                'available_plans': self._get_upgrade_options(query_type)
            }
        
        # Route to handler
        handler = self.handlers.get(query_type)
        if handler:
            try:
                return handler.process(query, user, context)
            except Exception as e:
                return {
                    'error': 'Processing error',
                    'message': '–í–∏–Ω–∏–∫–ª–∞ –ø–æ–º–∏–ª–∫–∞ –ø—Ä–∏ –æ–±—Ä–æ–±—Ü—ñ –∑–∞–ø–∏—Ç—É. –°–ø—Ä–æ–±—É–π—Ç–µ —â–µ —Ä–∞–∑.',
                    'debug_info': str(e) if user.is_admin else None
                }
        
        # Default fallback
        return {
            'type': 'general',
            'message': '–í–∏–±–∞—á—Ç–µ, –Ω–µ –∑–º—ñ–≥ –≤–∏–∑–Ω–∞—á–∏—Ç–∏ —Ç–∏–ø –∑–∞–ø–∏—Ç—É. –£—Ç–æ—á–Ω—ñ—Ç—å, –±—É–¥—å –ª–∞—Å–∫–∞.',
            'suggestions': [
                'üíä –ë–ê–î–∏ —Ç–∞ –¥–æ–±–∞–≤–∫–∏',
                'ü©∏ –ê–Ω–∞–ª—ñ–∑–∏ –∫—Ä–æ–≤—ñ',
                'üçé –•–∞—Ä—á—É–≤–∞–Ω–Ω—è —Ç–∞ —Å–ø–æ—Å—ñ–± –∂–∏—Ç—Ç—è',
                'üèÉ –¢—Ä–µ–Ω—É–≤–∞–Ω–Ω—è',
                'üß¨ Anti-aging —Ç–∞ –¥–æ–≤–≥–æ–ª—ñ—Ç—Ç—è'
            ]
        }
    
    def _get_upgrade_options(self, module_type):
        """–û–ø—Ü—ñ—ó –¥–ª—è –∞–ø–≥—Ä–µ–π–¥—É –ø—ñ–¥–ø–∏—Å–∫–∏"""
        if module_type == 'anti_aging':
            return {
                'required_plan': 'premium',
                'features': [
                    '–ü–µ—Ä—Å–æ–Ω–∞–ª—ñ–∑–æ–≤–∞–Ω—ñ anti-aging –ø—Ä–æ—Ç–æ–∫–æ–ª–∏',
                    'NAD+ —Ç–∞ –º—ñ—Ç–æ—Ö–æ–Ω–¥—Ä—ñ–∞–ª—å–Ω–∞ –æ–ø—Ç–∏–º—ñ–∑–∞—Ü—ñ—è',
                    '–ö–æ–≥–Ω—ñ—Ç–∏–≤–Ω–µ –ø–æ–∫—Ä–∞—â–µ–Ω–Ω—è',
                    '–ü—Ä–æ—Ç–æ–∫–æ–ª–∏ –¥–æ–≤–≥–æ–ª—ñ—Ç—Ç—è',
                    '–ö–æ–Ω—Å—É–ª—å—Ç–∞—Ü—ñ—ó –∑ —Å–µ–Ω–æ–ª—ñ—Ç–∏–∫–∞–º–∏'
                ],
                'price': '–≤—ñ–¥ 99 –≥—Ä–Ω/–º—ñ—Å'
            }
        return {}

# ===========================================
# 4. MATCHER UPDATES
# ===========================================

# utils/matcher.py (–û–Ω–æ–≤–ª–µ–Ω–Ω—è QueryMatcher)
class QueryMatcher:
    def __init__(self):
        self.patterns = {
            'supplements': [
                r'\b(–±–∞–¥|–¥–æ–±–∞–≤–∫|–≤—ñ—Ç–∞–º—ñ–Ω|–º—ñ–Ω–µ—Ä–∞–ª|–º–∞–≥–Ω—ñ–π|—Ü–∏–Ω–∫|–æ–º–µ–≥–∞)\b',
                r'\b(supplements|vitamins|minerals)\b'
            ],
            'steroids': [
                r'\b(–∫—É—Ä—Å|—Å—Ç–µ—Ä–æ—ó–¥|—Ç–µ—Å—Ç–æ—Å—Ç–µ—Ä–æ–Ω|–ø—Å—Ç|–ø–∫—Ç|–∞–Ω–∞–±–æ–ª)\b',
                r'\b(cycle|steroid|testosterone|pct|anabolic)\b'
            ],
            'blood': [
                r'\b(–∞–Ω–∞–ª—ñ–∑|–∫—Ä–æ–≤|–≥–æ—Ä–º–æ–Ω|–±—ñ–æ—Ö—ñ–º|–ø–æ–∫–∞–∑–Ω–∏–∫)\b',
                r'\b(blood|lab|hormone|biomarker)\b'
            ],
            'lifestyle': [
                r'\b(—Ö–∞—Ä—á—É–≤–∞–Ω–Ω—è|–¥—ñ—î—Ç–∞|—Ç—Ä–µ–Ω—É–≤–∞–Ω–Ω—è|—Å–æ–Ω|—Å—Ç—Ä–µ—Å)\b',
                r'\b(nutrition|diet|exercise|sleep|stress)\b'
            ],
            'womens_health': [
                r'\b(–∂—ñ–Ω–æ—á|–º–µ–Ω—Å—Ç—Ä—É–∞–ª|—Ü–∏–∫–ª|–≥–æ—Ä–º–æ–Ω|–µ—Å—Ç—Ä–æ–≥–µ–Ω)\b',
                r'\b(women|menstrual|cycle|estrogen|pms)\b'
            ],
            'anti_aging': [  # –î–û–î–ê–ù–û
                r'\b(—Å—Ç–∞—Ä—ñ–Ω–Ω—è|–¥–æ–≤–≥–æ–ª—ñ—Ç—Ç—è|–∞–Ω—Ç–∏–µ–π–¥–∂|–º–æ–ª–æ–¥—ñ—Å—Ç—å)\b',
                r'\b(aging|longevity|antiage|youth)\b',
                r'\b(nmn|nad|–º—ñ—Ç–æ—Ö–æ–Ω–¥—Ä—ñ|—Å–µ–Ω–æ–ª—ñ—Ç–∏–∫|–∞—É—Ç–æ—Ñ–∞–≥—ñ—è)\b',
                r'\b(mitochondria|senolytic|autophagy|resveratrol)\b',
                r'\b(–≥–æ–ª–æ–¥—É–≤–∞–Ω–Ω—è|—ñ–Ω—Ç–µ—Ä–≤–∞–ª—å–Ω|fasting|–ø—Ä–æ—Ç–æ–∫–æ–ª)\b',
                r'\b(–Ω–æ–æ—Ç—Ä–æ–ø|–∫–æ–≥–Ω—ñ—Ç–∏–≤|lion|alpha-gpc|—Ñ—ñ—Å–µ—Ç–∏–Ω)\b'
            ]
        }
        
        # –í–∞–≥–æ–≤—ñ –∫–æ–µ—Ñ—ñ—Ü—ñ—î–Ω—Ç–∏ –¥–ª—è –∫–∞—Ç–µ–≥–æ—Ä—ñ–π
        self.category_weights = {
            'anti_aging': 1.2,  # –¢—Ä–æ—Ö–∏ –≤–∏—â–∞ –≤–∞–≥–∞ –¥–ª—è anti-aging
            'supplements': 1.0,
            'steroids': 1.1,
            'blood': 1.0,
            'lifestyle': 0.9,
            'womens_health': 1.1
        }
    
    def classify_query(self, query):
        """–ö–ª–∞—Å–∏—Ñ—ñ–∫–∞—Ü—ñ—è –∑–∞–ø–∏—Ç—É –∑–∞ —Ç–∏–ø–æ–º –∑ —É—Ä–∞—Ö—É–≤–∞–Ω–Ω—è–º –≤–∞–≥"""
        query_lower = query.lower()
        scores = {}
        
        for category, patterns in self.patterns.items():
            score = 0
            for pattern in patterns:
                matches = len(re.findall(pattern, query_lower, re.IGNORECASE))
                score += matches
            
            # –ó–∞—Å—Ç–æ—Å—É–≤–∞–Ω–Ω—è –≤–∞–≥–æ–≤–∏—Ö –∫–æ–µ—Ñ—ñ—Ü—ñ—î–Ω—Ç—ñ–≤
            weight = self.category_weights.get(category, 1.0)
            scores[category] = score * weight
        
        # Return category with highest score
        if max(scores.values()) > 0:
            return max(scores, key=scores.get)
        
        return 'general'

# ===========================================
# 5. ROUTES UPDATES
# ===========================================

# routes/assistant.py (–î–æ–¥–∞—Ç–∫–∏ –¥–æ —ñ—Å–Ω—É—é—á–∏—Ö —Ä–æ—É—Ç—ñ–≤)

@bp.route('/anti-aging/categories', methods=['GET'])
@require_auth
def get_antiaging_categories():
    """–ö–∞—Ç–µ–≥–æ—Ä—ñ—ó anti-aging –º–æ–¥—É–ª—è"""
    
    if not g.current_user.has_access('anti_aging'):
        return jsonify({'error': 'Premium subscription required'}), 402
    
    categories = {
        'nad_energy': {
            'title': 'NAD+ —Ç–∞ –µ–Ω–µ—Ä–≥—ñ—è',
            'emoji': '‚ö°',
            'description': '–ú—ñ—Ç–æ—Ö–æ–Ω–¥—Ä—ñ–∞–ª—å–Ω–∞ –µ–Ω–µ—Ä–≥—ñ—è, NAD+ –±—É—Å—Ç–µ—Ä–∏, CoQ10',
            'keywords': ['nmn', 'nad', 'coq10', '–º—ñ—Ç–æ—Ö–æ–Ω–¥—Ä—ñ—ó', '–µ–Ω–µ—Ä–≥—ñ—è']
        },
        'cognitive': {
            'title': '–ó–¥–æ—Ä–æ–≤\'—è –º–æ–∑–∫—É',
            'emoji': 'üß†',
            'description': '–ù–æ–æ—Ç—Ä–æ–ø–∏, –∫–æ–≥–Ω—ñ—Ç–∏–≤–Ω—ñ —Ñ—É–Ω–∫—Ü—ñ—ó, –Ω–µ–π—Ä–æ–ø–ª–∞—Å—Ç–∏—á–Ω—ñ—Å—Ç—å',
            'keywords': ['–Ω–æ–æ—Ç—Ä–æ–ø', '–ø–∞–º\'—è—Ç—å', '—Ñ–æ–∫—É—Å', 'lion mane', 'alpha-gpc']
        },
        'fasting': {
            'title': '–ü—Ä–æ—Ç–æ–∫–æ–ª–∏ –≥–æ–ª–æ–¥—É–≤–∞–Ω–Ω—è',
            'emoji': '‚è∞',
            'description': '–Ü–Ω—Ç–µ—Ä–≤–∞–ª—å–Ω–µ –≥–æ–ª–æ–¥—É–≤–∞–Ω–Ω—è, –∞—É—Ç–æ—Ñ–∞–≥—ñ—è, –∫–µ—Ç–æ–∑',
            'keywords': ['–≥–æ–ª–æ–¥—É–≤–∞–Ω–Ω—è', '16:8', '–æ–º–∞–¥', '–∞—É—Ç–æ—Ñ–∞–≥—ñ—è', '–∫–µ—Ç–æ–∑']
        },
        'senolytics': {
            'title': '–°–µ–Ω–æ–ª—ñ—Ç–∏–∫–∏',
            'emoji': 'üßü',
            'description': '–û—á–∏—â–µ–Ω–Ω—è —Å—Ç–∞—Ä—ñ—é—á–∏—Ö –∫–ª—ñ—Ç–∏–Ω, –ø–µ–ø—Ç–∏–¥–∏',
            'keywords': ['—Ñ—ñ—Å–µ—Ç–∏–Ω', '–∫–≤–µ—Ä—Ü–µ—Ç–∏–Ω', '—Å–µ–Ω–æ–ª—ñ—Ç–∏–∫', '–µ–ø—ñ—Ç–∞–ª–æ–Ω']
        },
        'sleep': {
            'title': '–°–æ–Ω —Ç–∞ –≤—ñ–¥–Ω–æ–≤–ª–µ–Ω–Ω—è',
            'emoji': 'üò¥',
            'description': '–û–ø—Ç–∏–º—ñ–∑–∞—Ü—ñ—è —Å–Ω—É, –¥–æ–±–∞–≤–∫–∏ –¥–ª—è –≤—ñ–¥–Ω–æ–≤–ª–µ–Ω–Ω—è',
            'keywords': ['—Å–æ–Ω', '–º–µ–ª–∞—Ç–æ–Ω—ñ–Ω', '–º–∞–≥–Ω—ñ–π', '–≤—ñ–¥–Ω–æ–≤–ª–µ–Ω–Ω—è']
        },
        'protocols': {
            'title': '–ö–æ–º–ø–ª–µ–∫—Å–Ω—ñ –ø—Ä–æ—Ç–æ–∫–æ–ª–∏',
            'emoji': 'üìã',
            'description': '–ì–æ—Ç–æ–≤—ñ —Å—Ö–µ–º–∏ —Ç–∞ —Å—Ç–µ–∫–∏ –¥–ª—è —Ä—ñ–∑–Ω–∏—Ö —Ü—ñ–ª–µ–π',
            'keywords': ['–ø—Ä–æ—Ç–æ–∫–æ–ª', '—Å—Ç–µ–∫', '—Å—Ö–µ–º–∞', '–ø–ª–∞–Ω']
        }
    }
    return jsonify(categories)

@bp.route('/anti-aging/query', methods=['POST'])
@require_auth
def process_antiaging_query():
    """–°–ø–µ—Ü—ñ–∞–ª—å–Ω–∏–π –µ–Ω–¥–ø–æ—ñ–Ω—Ç –¥–ª—è anti-aging –∑–∞–ø–∏—Ç—ñ–≤"""
    user = g.current_user
    data = request.json
    
    if not user.has_access('anti_aging'):
        return jsonify({
            'error': 'Premium subscription required',
            'upgrade_info': {
                'required_plan': 'premium',
                'benefits': [
                    '–ü–æ–≤–Ω–∏–π –¥–æ—Å—Ç—É–ø –¥–æ anti-aging –º–æ–¥—É–ª—è',
                    '–ü–µ—Ä—Å–æ–Ω–∞–ª—ñ–∑–æ–≤–∞–Ω—ñ –ø—Ä–æ—Ç–æ–∫–æ–ª–∏ –¥–æ–≤–≥–æ–ª—ñ—Ç—Ç—è',
                    'NAD+ —Ç–∞ –º—ñ—Ç–æ—Ö–æ–Ω–¥—Ä—ñ–∞–ª—å–Ω–∞ –æ–ø—Ç–∏–º—ñ–∑–∞—Ü—ñ—è',
                    '–ö–æ–≥–Ω—ñ—Ç–∏–≤–Ω–µ –ø–æ–∫—Ä–∞—â–µ–Ω–Ω—è —Ç–∞ –Ω–æ–æ—Ç—Ä–æ–ø–∏',
                    '–°–µ–Ω–æ–ª—ñ—Ç–∏–∫–∏ —Ç–∞ –ø–µ–ø—Ç–∏–¥–Ω–∞ —Ç–µ—Ä–∞–ø—ñ—è'
                ]
            }
        }), 402
    
    query = clean_text(data.get('message', ''))
    category = data.get('category', 'general')  # –û–ø—Ü—ñ–æ–Ω–∞–ª—å–Ω–∞ –∫–∞—Ç–µ–≥–æ—Ä—ñ—è
    
    try:
        handler = AntiAgingHandler()
        
        # –î–æ–¥–∞—Ç–∏ –∫–∞—Ç–µ–≥–æ—Ä—ñ—é –≤ –∫–æ–Ω—Ç–µ–∫—Å—Ç —è–∫—â–æ –≤–∫–∞–∑–∞–Ω–∞
        context = {'category': category} if category != 'general' else {}
        
        response = handler.process(query, user, context)
        
        # –ó–±–µ—Ä–µ–≥—Ç–∏ –≤–∑–∞—î–º–æ–¥—ñ—é
        user.add_interaction(query, response)
        
        return jsonify(response)
    
    except Exception as e:
        return jsonify({
            'error': 'Processing error',
            'message': '–í–∏–Ω–∏–∫–ª–∞ –ø–æ–º–∏–ª–∫–∞ –ø—Ä–∏ –æ–±—Ä–æ–±—Ü—ñ –∑–∞–ø–∏—Ç—É –ø—Ä–æ anti-aging',
            'suggestion': '–°–ø—Ä–æ–±—É–π—Ç–µ –ø–µ—Ä–µ—Ñ–æ—Ä–º—É–ª—é–≤–∞—Ç–∏ –∑–∞–ø–∏—Ç –∞–±–æ –æ–±–µ—Ä—ñ—Ç—å –∫–æ–Ω–∫—Ä–µ—Ç–Ω—É –∫–∞—Ç–µ–≥–æ—Ä—ñ—é'
        }), 500

@bp.route('/anti-aging/recommendations/<category>', methods=['GET'])
@require_auth  
def get_antiaging_recommendations(category):
    """–†–µ–∫–æ–º–µ–Ω–¥–∞—Ü—ñ—ó –∑–∞ –∫–∞—Ç–µ–≥–æ—Ä—ñ—è–º–∏"""
    user = g.current_user
    
    if not user.has_access('anti_aging'):
        return jsonify({'error': 'Premium subscription required'}), 402
    
    valid_categories = ['nad_energy', 'cognitive', 'fasting', 'senolytics', 'sleep', 'protocols']
    
    if category not in valid_categories:
        return jsonify({'error': 'Invalid category'}), 400
    
    try:
        handler = AntiAgingHandler()
        
        # –°—Ç–≤–æ—Ä–∏—Ç–∏ –∑–∞–ø–∏—Ç –¥–ª—è –∫–∞—Ç–µ–≥–æ—Ä—ñ—ó
        category_queries = {
            'nad_energy': '–Ø–∫ –ø—ñ–¥–≤–∏—â–∏—Ç–∏ –µ–Ω–µ—Ä–≥—ñ—é —Ç–∞ NAD+?',
            'cognitive': '–ù–æ–æ—Ç—Ä–æ–ø–∏ –¥–ª—è –ø–æ–∫—Ä–∞—â–µ–Ω–Ω—è –∫–æ–≥–Ω—ñ—Ç–∏–≤–Ω–∏—Ö —Ñ—É–Ω–∫—Ü—ñ–π',
            'fasting': '–ü—Ä–æ—Ç–æ–∫–æ–ª–∏ —ñ–Ω—Ç–µ—Ä–≤–∞–ª—å–Ω–æ–≥–æ –≥–æ–ª–æ–¥—É–≤–∞–Ω–Ω—è',
            'senolytics': '–°–µ–Ω–æ–ª—ñ—Ç–∏–∫–∏ –¥–ª—è –æ—á–∏—â–µ–Ω–Ω—è —Å—Ç–∞—Ä—ñ—é—á–∏—Ö –∫–ª—ñ—Ç–∏–Ω',
            'sleep': '–î–æ–±–∞–≤–∫–∏ –¥–ª—è –ø–æ–∫—Ä–∞—â–µ–Ω–Ω—è —Å–Ω—É',
            'protocols': '–ö–æ–º–ø–ª–µ–∫—Å–Ω—ñ anti-aging –ø—Ä–æ—Ç–æ–∫–æ–ª–∏'
        }
        
        query = category_queries.get(category, f'–Ü–Ω—Ñ–æ—Ä–º–∞—Ü—ñ—è –ø—Ä–æ {category}')
        response = handler.process(query, user, {'category': category})
        
        return jsonify(response)
    
    except Exception as e:
        return jsonify({'error': 'Failed to get recommendations'}), 500

# ===========================================
# 6. USER MODEL UPDATES
# ===========================================

# models/user.py (–û–Ω–æ–≤–ª–µ–Ω–Ω—è access matrix)
def has_access(self, module):
    """–ü–µ—Ä–µ–≤—ñ—Ä–∫–∞ –¥–æ—Å—Ç—É–ø—É –¥–æ –º–æ–¥—É–ª—è"""
    if not self.has_active_subscription():
        return module in ['supplements', 'lifestyle']  # –¢—Ä—ñ–∞–ª –¥–æ—Å—Ç—É–ø
    
    plan = self.subscription.get('plan', 'trial')
    
    access_matrix = {
        'trial': ['supplements', 'lifestyle'],
        'basic': ['supplements', 'lifestyle', 'blood'],
        'pro': ['supplements', 'lifestyle', 'blood', 'steroids', 'womens_health'],
        'premium': ['supplements', 'lifestyle', 'blood', 'steroids', 'womens_health', 'anti_aging']  # –î–û–î–ê–ù–û
    }
    
    return module in access_matrix.get(plan, [])

def get_subscription_features(self):
    """–û—Ç—Ä–∏–º–∞—Ç–∏ —Å–ø–∏—Å–æ–∫ –¥–æ—Å—Ç—É–ø–Ω–∏—Ö —Ñ—É–Ω–∫—Ü—ñ–π"""
    plan = self.subscription.get('plan', 'trial')
    
    features_matrix = {
        'trial': {
            'modules': ['supplements', 'lifestyle'],
            'limits': {'queries_per_day': 10},
            'description': '–ë–∞–∑–æ–≤–∏–π –¥–æ—Å—Ç—É–ø –¥–æ –¥–æ–±–∞–≤–æ–∫ —Ç–∞ —Å–ø–æ—Å–æ–±—É –∂–∏—Ç—Ç—è'
        },
        'basic': {
            'modules': ['supplements', 'lifestyle', 'blood'],
            'limits': {'queries_per_day': 50},
            'description': '–î–æ—Å—Ç—É–ø –¥–æ –∞–Ω–∞–ª—ñ–∑—ñ–≤ –∫—Ä–æ–≤—ñ'
        },
        'pro': {
            'modules': ['supplements', 'lifestyle', 'blood', 'steroids', 'womens_health'],
            'limits': {'queries_per_day': 100},
            'description': '–ü–æ–≤–Ω–∏–π –¥–æ—Å—Ç—É–ø –∫—Ä—ñ–º anti-aging'
        },
        'premium': {
            'modules': ['supplements', 'lifestyle', 'blood', 'steroids', 'womens_health', 'anti_aging'],
            'limits': {'queries_per_day': 200},
            'description': '–ü–æ–≤–Ω–∏–π –¥–æ—Å—Ç—É–ø –≤–∫–ª—é—á–Ω–æ –∑ anti-aging'
        }
    }
    
    return features_matrix.get(plan, features_matrix['trial'])

# ===========================================
# 7. JSON FILES CREATION SCRIPT
# ===========================================

def create_antiaging_json_files():
    """–°–∫—Ä–∏–ø—Ç –¥–ª—è —Å—Ç–≤–æ—Ä–µ–Ω–Ω—è JSON —Ñ–∞–π–ª—ñ–≤"""
    
    import os
    import json
    from pathlib import Path
    
    # –°—Ç–≤–æ—Ä–∏—Ç–∏ –¥–∏—Ä–µ–∫—Ç–æ—Ä—ñ—é
    data_dir = Path('data/anti_aging')
    data_dir.mkdir(parents=True, exist_ok=True)
    
    # JSON –¥–∞–Ω—ñ
    json_files = {
        'aging_theories.json': AGING_THEORIES_JSON,
        'nad_mitochondrial.json': NAD_MITOCHONDRIAL_JSON,
        'cognitive_enhancement.json': COGNITIVE_ENHANCEMENT_JSON,
        'fasting_protocols.json': FASTING_PROTOCOLS_JSON,
        'senolytics_peptides.json': SENOLYTICS_PEPTIDES_JSON,
        'sleep_restoration.json': SLEEP_RESTORATION_JSON,
        'comprehensive_protocols.json': COMPREHENSIVE_PROTOCOLS_JSON
    }
    
    # –ó–∞–ø–∏—Å–∞—Ç–∏ —Ñ–∞–π–ª–∏
    for filename, json_content in json_files.items():
        file_path = data_dir / filename
        try:
            data = json.loads(json_content)
            with open(file_path, 'w', encoding='utf-8') as f:
                json.dump(data, f, ensure_ascii=False, indent=2)
            print(f"‚úÖ Created {filename}")
        except Exception as e:
            print(f"‚ùå Error creating {filename}: {e}")

# ===========================================
# 8. TESTING AND VALIDATION
# ===========================================

def test_antiaging_handler():
    """–¢–µ—Å—Ç—É–≤–∞–Ω–Ω—è AntiAgingHandler"""
    
    handler = AntiAgingHandler()
    
    # –¢–µ—Å—Ç–æ–≤—ñ –∑–∞–ø–∏—Ç–∏
    test_queries = [
        ("–Ø–∫ –ø—ñ–¥–≤–∏—â–∏—Ç–∏ –µ–Ω–µ—Ä–≥—ñ—é?", "nad_energy"),
        ("NMN –¥–æ–∑—É–≤–∞–Ω–Ω—è", "nad_energy"),
        ("–ù–æ–æ—Ç—Ä–æ–ø–∏ –¥–ª—è –ø–∞–º'—è—Ç—ñ", "cognitive"),
        ("Lion's Mane –µ—Ñ–µ–∫—Ç–∏", "cognitive"), 
        ("–Ü–Ω—Ç–µ—Ä–≤–∞–ª—å–Ω–µ –≥–æ–ª–æ–¥—É–≤–∞–Ω–Ω—è 16:8", "fasting"),
        ("–§—ñ—Å–µ—Ç–∏–Ω —è–∫ —Å–µ–Ω–æ–ª—ñ—Ç–∏–∫", "senolytics"),
        ("–ú–∞–≥–Ω—ñ–π –¥–ª—è —Å–Ω—É", "sleep"),
        ("Anti-aging –ø—Ä–æ—Ç–æ–∫–æ–ª", "protocols"),
        ("–¢–µ–æ—Ä—ñ—ó —Å—Ç–∞—Ä—ñ–Ω–Ω—è", "theories"),
        ("–©–æ —Ç–∞–∫–µ anti-aging?", "general")
    ]
    
    print("üß™ Testing AntiAgingHandler Classification:")
    for query, expected in test_queries:
        result = handler.classify_antiaging_query(query)
        status = "‚úÖ" if result == expected else "‚ùå"
        print(f"{status} '{query}' -> {result} (expected: {expected})")
    
    # –¢–µ—Å—Ç –æ–±—Ä–æ–±–∫–∏
    print("\nüß™ Testing Processing:")
    user_mock = type('User', (), {'has_access': lambda x: True})()
    
    try:
        response = handler.process("–Ø–∫ –ø—ñ–¥–≤–∏—â–∏—Ç–∏ NAD+?", user_mock, {})
        print(f"‚úÖ NAD+ query processed: {response['title']}")
    except Exception as e:
        print(f"‚ùå Processing error: {e}")

if __name__ == "__main__":
    # –°—Ç–≤–æ—Ä–∏—Ç–∏ JSON —Ñ–∞–π–ª–∏
    print("üìÅ Creating Anti-Aging JSON files...")
    create_antiaging_json_files()
    
    # –ó–∞–ø—É—Å—Ç–∏—Ç–∏ —Ç–µ—Å—Ç–∏
    print("\nüß™ Running tests...")
    test_antiaging_handler()
    
    print("\nüöÄ Anti-Aging module integration complete!")
    print("\nNext steps:")
    print("1. ‚úÖ JSON files created in data/anti_aging/")
    print("2. ‚úÖ AntiAgingHandler implemented")  
    print("3. ‚úÖ Router and Matcher updated")
    print("4. ‚úÖ API endpoints added")
