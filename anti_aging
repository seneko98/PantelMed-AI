      }
    ],
    "money_saving_tips": [
      "–ö—É–ø—É–≤–∞—Ç–∏ –æ–ø—Ç–æ–º (3-6 –º—ñ—Å—è—á–Ω—ñ –∑–∞–ø–∞—Å–∏)",
      "–ü–æ—Ä—ñ–≤–Ω—é–≤–∞—Ç–∏ —Ü—ñ–Ω–∏ —Ä—ñ–∑–Ω–∏—Ö –±—Ä–µ–Ω–¥—ñ–≤",
      "–¶–∏–∫–ª—É–≤–∞–Ω–Ω—è –¥–æ—Ä–æ–≥–∏—Ö –¥–æ–±–∞–≤–æ–∫",
      "–ü—Ä—ñ–æ—Ä–∏—Ç–µ—Ç –Ω–∞ –¥–æ–∫–∞–∑–æ–≤—É –º–µ–¥–∏—Ü–∏–Ω—É",
      "–Ü–Ω–≤–µ—Å—Ç–∏—Ü—ñ—ó –≤ —Å–ø–æ—Å—ñ–± –∂–∏—Ç—Ç—è > –¥–æ–±–∞–≤–∫–∏"
    ]
  }
}
'''

# ===========================================
# 2. ANTI-AGING HANDLER
# ===========================================

import json
import os
from pathlib import Path
from typing import Dict, List, Any, Optional

class AntiAgingHandler:
    """–û–±—Ä–æ–±–Ω–∏–∫ –∑–∞–ø–∏—Ç—ñ–≤ –ø–æ anti-aging –º–æ–¥—É–ª—é"""
    
    def __init__(self):
        self.data_path = Path('data/anti_aging')
        self.data = {}
        self.load_all_data()
    
    def load_all_data(self):
        """–ó–∞–≤–∞–Ω—Ç–∞–∂–µ–Ω–Ω—è –≤—Å—ñ—Ö JSON —Ñ–∞–π–ª—ñ–≤ –∑ –¥–∞–Ω–∏–º–∏"""
        data_files = {
            'aging_theories': 'aging_theories.json',
            'nad_mitochondrial': 'nad_mitochondrial.json', 
            'cognitive_enhancement': 'cognitive_enhancement.json',
            'fasting_protocols': 'fasting_protocols.json',
            'senolytics_peptides': 'senolytics_peptides.json',
            'sleep_restoration': 'sleep_restoration.json',
            'comprehensive_protocols': 'comprehensive_protocols.json'
        }
        
        for key, filename in data_files.items():
            try:
                file_path = self.data_path / filename
                if file_path.exists():
                    with open(file_path, 'r', encoding='utf-8') as f:
                        self.data[key] = json.load(f)
                else:
                    print(f"Warning: {filename} not found, using default data")
                    self.data[key] = self._get_default_data(key)
            except Exception as e:
                print(f"Error loading {filename}: {e}")
                self.data[key] = {}
    
    def _get_default_data(self, data_type: str) -> Dict:
        """Fallback –¥–∞–Ω—ñ —è–∫—â–æ —Ñ–∞–π–ª–∏ –Ω–µ –∑–∞–≤–∞–Ω—Ç–∞–∂–∏–ª–∏—Å—å"""
        defaults = {
            'aging_theories': json.loads(AGING_THEORIES_JSON),
            'nad_mitochondrial': json.loads(NAD_MITOCHONDRIAL_JSON),
            'cognitive_enhancement': json.loads(COGNITIVE_ENHANCEMENT_JSON),
            'fasting_protocols': json.loads(FASTING_PROTOCOLS_JSON),
            'senolytics_peptides': json.loads(SENOLYTICS_PEPTIDES_JSON),
            'sleep_restoration': json.loads(SLEEP_RESTORATION_JSON),
            'comprehensive_protocols': json.loads(COMPREHENSIVE_PROTOCOLS_JSON)
        }
        return defaults.get(data_type, {})
    
    def process(self, query: str, user, context: Dict) -> Dict:
        """–û—Å–Ω–æ–≤–Ω–∏–π –º–µ—Ç–æ–¥ –æ–±—Ä–æ–±–∫–∏ anti-aging –∑–∞–ø–∏—Ç—ñ–≤"""
        
        # –ö–ª–∞—Å–∏—Ñ—ñ–∫–∞—Ü—ñ—è —Ç–∏–ø—É –∑–∞–ø–∏—Ç—É
        query_type = self.classify_antiaging_query(query)
        
        # –†–æ—É—Ç–∏–Ω–≥ –¥–æ –≤—ñ–¥–ø–æ–≤—ñ–¥–Ω–æ–≥–æ –æ–±—Ä–æ–±–Ω–∏–∫–∞
        handlers = {
            'nad_energy': self.handle_nad_energy,
            'cognitive': self.handle_cognitive,
            'fasting': self.handle_fasting,
            'senolytics': self.handle_senolytics,
            'sleep': self.handle_sleep,
            'protocols': self.handle_protocols,
            'theories': self.handle_theories,
            'general': self.handle_general_antiaging
        }
        
        handler = handlers.get(query_type, self.handle_general_antiaging)
        return handler(query, user, context)
    
    def classify_antiaging_query(self, query: str) -> str:
        """–ö–ª–∞—Å–∏—Ñ—ñ–∫–∞—Ü—ñ—è anti-aging –∑–∞–ø–∏—Ç—ñ–≤ –∑–∞ –∫–∞—Ç–µ–≥–æ—Ä—ñ—è–º–∏"""
        query_lower = query.lower()
        
        # NAD+ —Ç–∞ –µ–Ω–µ—Ä–≥—ñ—è
        if any(word in query_lower for word in [
            'nmn', 'nad', '–º—ñ—Ç–æ—Ö–æ–Ω–¥—Ä—ñ', '–µ–Ω–µ—Ä–≥—ñ—è', '–≤—Ç–æ–º–∞', 'coq10', 'pqq', 'urolithin'
        ]):
            return 'nad_energy'
        
        # –ö–æ–≥–Ω—ñ—Ç–∏–≤–Ω—ñ —Ñ—É–Ω–∫—Ü—ñ—ó  
        elif any(word in query_lower for word in [
            '–º–æ–∑–æ–∫', '–ø–∞–º\'—è—Ç—å', '—Ñ–æ–∫—É—Å', '–∫–æ–≥–Ω—ñ—Ç–∏–≤', '–Ω–æ–æ—Ç—Ä–æ–ø', 'lion', 'alpha-gpc'
        ]):
            return 'cognitive'
        
        # –ì–æ–ª–æ–¥—É–≤–∞–Ω–Ω—è
        elif any(word in query_lower for word in [
            '–≥–æ–ª–æ–¥—É–≤–∞–Ω–Ω—è', '—ñ–Ω—Ç–µ—Ä–≤–∞–ª—å–Ω', 'fasting', '–∞—É—Ç–æ—Ñ–∞–≥—ñ—è', '16:8', 'omad'
        ]):
            return 'fasting'
        
        # –°–µ–Ω–æ–ª—ñ—Ç–∏–∫–∏
        elif any(word in query_lower for word in [
            '—Å–µ–Ω–æ–ª—ñ—Ç–∏–∫', '—Ñ—ñ—Å–µ—Ç–∏–Ω', '–∫–≤–µ—Ä—Ü–µ—Ç–∏–Ω', '—Å—Ç–∞—Ä—ñ—é—á', '–∫–ª—ñ—Ç–∏–Ω'
        ]):
            return 'senolytics'
        
        # –°–æ–Ω
        elif any(word in query_lower for word in [
            '—Å–æ–Ω', '–±–µ–∑—Å–æ–Ω–Ω—è', '–º–µ–ª–∞—Ç–æ–Ω—ñ–Ω', '–º–∞–≥–Ω—ñ–π', '–≤—ñ–¥–Ω–æ–≤–ª–µ–Ω–Ω—è'
        ]):
            return 'sleep'
        
        # –ü—Ä–æ—Ç–æ–∫–æ–ª–∏
        elif any(word in query_lower for word in [
            '–ø—Ä–æ—Ç–æ–∫–æ–ª', '—Å—Ö–µ–º–∞', '–ø–ª–∞–Ω', '–∫–æ–º–ø–ª–µ–∫—Å', '—Å—Ç–µ–∫'
        ]):
            return 'protocols'
        
        # –¢–µ–æ—Ä—ñ—ó —Å—Ç–∞—Ä—ñ–Ω–Ω—è
        elif any(word in query_lower for word in [
            '—Ç–µ–æ—Ä—ñ—è', '–º–µ—Ö–∞–Ω—ñ–∑–º', '—Å—Ç–∞—Ä—ñ–Ω–Ω—è', '–ø—Ä–∏—á–∏–Ω'
        ]):
            return 'theories'
        
        else:
            return 'general'
    
    def handle_nad_energy(self, query: str, user, context: Dict) -> Dict:
        """–û–±—Ä–æ–±–∫–∞ –∑–∞–ø–∏—Ç—ñ–≤ –ø—Ä–æ NAD+ —Ç–∞ –µ–Ω–µ—Ä–≥—ñ—é"""
        
        nad_data = self.data.get('nad_mitochondrial', {})
        
        # NAD+ –±—É—Å—Ç–µ—Ä–∏
        nad_boosters = []
        for supplement in nad_data.get('nad_boosters', {}).get('supplements', []):
            nad_boosters.append({
                'name': supplement['name'],
                'full_name': supplement.get('full_name', ''),
                'dosage': supplement['dosage'],
                'timing': supplement.get('timing', ''),
                'effects': supplement['effects'],
                'price': supplement.get('price_range', '—É—Ç–æ—á–Ω–∏—Ç–∏'),
                'mechanism': supplement.get('mechanism', ''),
                'evidence': supplement.get('evidence_level', 'medium')
            })
        
        # –ú—ñ—Ç–æ—Ö–æ–Ω–¥—Ä—ñ–∞–ª—å–Ω–∞ –ø—ñ–¥—Ç—Ä–∏–º–∫–∞
        mito_support = []
        for supplement in nad_data.get('mitochondrial_support', {}).get('supplements', []):
            mito_support.append({
                'name': supplement['name'],
                'dosage': supplement['dosage'],
                'effects': supplement['effects'],
                'price': supplement.get('price_range', '—É—Ç–æ—á–Ω–∏—Ç–∏'),
                'mechanism': supplement.get('mechanism', ''),
                'note': supplement.get('note', '')
            })
        
        # –ü—Ä–æ—Ç–æ–∫–æ–ª–∏ –µ–Ω–µ—Ä–≥—ñ—ó
        energy_protocols = nad_data.get('energy_optimization', {}).get('protocols', [])
        
        return {
            'type': 'nad_energy',
            'title': '‚ö° NAD+ —Ç–∞ –∫–ª—ñ—Ç–∏–Ω–Ω–∞ –µ–Ω–µ—Ä–≥—ñ—è',
            'description': '–í—ñ–¥–Ω–æ–≤–ª–µ–Ω–Ω—è –µ–Ω–µ—Ä–≥–µ—Ç–∏—á–Ω–æ–≥–æ –ø–æ—Ç–µ–Ω—Ü—ñ–∞–ª—É –∫–ª—ñ—Ç–∏–Ω',
            'nad_boosters': nad_boosters,
            'mitochondrial_support': mito_support,
            'energy_protocols': energy_protocols,
            'key_benefits': [
                'üîã –ü—ñ–¥–≤–∏—â–µ–Ω–Ω—è –∫–ª—ñ—Ç–∏–Ω–Ω–æ—ó –µ–Ω–µ—Ä–≥—ñ—ó',
                'üß¨ –ê–∫—Ç–∏–≤–∞—Ü—ñ—è sirtuins (–≥–µ–Ω–∏ –¥–æ–≤–≥–æ–ª—ñ—Ç—Ç—è)',
                '‚öôÔ∏è –ü–æ–∫—Ä–∞—â–µ–Ω–Ω—è –º—ñ—Ç–æ—Ö–æ–Ω–¥—Ä—ñ–∞–ª—å–Ω–æ—ó —Ñ—É–Ω–∫—Ü—ñ—ó',
                'üîÑ –°—Ç–∏–º—É–ª—è—Ü—ñ—è —Ä–µ–ø–∞—Ä–∞—Ü—ñ—ó –î–ù–ö'
            ],
            'implementation_tips': [
                '–ü–æ—á–Ω—ñ—Ç—å –∑ CoQ10 —è–∫ –æ—Å–Ω–æ–≤–∏',
                '–î–æ–¥–∞–π—Ç–µ NMN –∞–±–æ NR —á–µ—Ä–µ–∑ 2 —Ç–∏–∂–Ω—ñ',
