    def handle_protocols(self, query: str, user, context: Dict) -> Dict:
        """–û–±—Ä–æ–±–∫–∞ –∑–∞–ø–∏—Ç—ñ–≤ –ø—Ä–æ –∫–æ–º–ø–ª–µ–∫—Å–Ω—ñ –ø—Ä–æ—Ç–æ–∫–æ–ª–∏"""
        
        protocols_data = self.data.get('comprehensive_protocols', {})
        
        # –ü—Ä–æ—Ç–æ–∫–æ–ª–∏ –∑–∞ —Ä—ñ–≤–Ω—è–º–∏
        basic_protocol = protocols_data.get('basic_longevity_stack', {})
        intermediate_protocol = protocols_data.get('intermediate_longevity_protocol', {})
        advanced_protocol = protocols_data.get('advanced_longevity_protocol', {})
        
        # –¶—ñ–ª—å–æ–≤—ñ –ø—Ä–æ—Ç–æ–∫–æ–ª–∏
        targeted_protocols = protocols_data.get('targeted_protocols', {})
        
        # –ü–æ—Å—ñ–±–Ω–∏–∫ –≤–ø—Ä–æ–≤–∞–¥–∂–µ–Ω–Ω—è
        implementation_guide = protocols_data.get('implementation_guide', {})
        
        # –û–ø—Ç–∏–º—ñ–∑–∞—Ü—ñ—è –≤–∞—Ä—Ç–æ—Å—Ç—ñ
        cost_optimization = protocols_data.get('cost_optimization', {})
        
        return {
            'type': 'protocols',
            'title': 'üìã –ö–æ–º–ø–ª–µ–∫—Å–Ω—ñ anti-aging –ø—Ä–æ—Ç–æ–∫–æ–ª–∏',
            'description': '–ì–æ—Ç–æ–≤—ñ —Å—Ö–µ–º–∏ –¥–ª—è —Ä—ñ–∑–Ω–∏—Ö —Ä—ñ–≤–Ω—ñ–≤ —Ç–∞ —Ü—ñ–ª–µ–π',
            'basic_protocol': basic_protocol,
            'intermediate_protocol': intermediate_protocol,
            'advanced_protocol': advanced_protocol,
            'targeted_protocols': targeted_protocols,
            'implementation_guide': implementation_guide,
            'cost_optimization': cost_optimization,
            'protocol_selection': [
                'üü¢ –ë–∞–∑–æ–≤–∏–π (1500-2500 –≥—Ä–Ω/–º—ñ—Å) - –æ—Å–Ω–æ–≤–∏ –∑–¥–æ—Ä–æ–≤\'—è',
                'üü° –ü—Ä–æ–º—ñ–∂–Ω–∏–π (2500-4000 –≥—Ä–Ω/–º—ñ—Å) - –∞–∫—Ç–∏–≤–Ω–∞ –ø—Ä–æ—Ñ—ñ–ª–∞–∫—Ç–∏–∫–∞',
                'üî¥ –ü—Ä–æ—Å—É–Ω—É—Ç–∏–π (4000-7000 –≥—Ä–Ω/–º—ñ—Å) - –º–∞–∫—Å–∏–º–∞–ª—å–Ω–∞ –æ–ø—Ç–∏–º—ñ–∑–∞—Ü—ñ—è'
            ],
            'success_factors': [
                'üìà –ü–æ—Å—Ç—É–ø–æ–≤–µ –≤–ø—Ä–æ–≤–∞–¥–∂–µ–Ω–Ω—è',
                'üîÑ –†–µ–≥—É–ª—è—Ä–Ω—ñ—Å—Ç—å –≤–∞–∂–ª–∏–≤—ñ—à–µ —ñ–Ω—Ç–µ–Ω—Å–∏–≤–Ω–æ—Å—Ç—ñ',
                'üìä –ú–æ–Ω—ñ—Ç–æ—Ä–∏–Ω–≥ —Ä–µ–∑—É–ª—å—Ç–∞—Ç—ñ–≤',
                'üéØ –ü–µ—Ä—Å–æ–Ω–∞–ª—ñ–∑–∞—Ü—ñ—è –ø—ñ–¥ —Å–≤–æ—ó –ø–æ—Ç—Ä–µ–±–∏'
            ]
        }
    
    def handle_theories(self, query: str, user, context: Dict) -> Dict:
        """–û–±—Ä–æ–±–∫–∞ –∑–∞–ø–∏—Ç—ñ–≤ –ø—Ä–æ —Ç–µ–æ—Ä—ñ—ó —Å—Ç–∞—Ä—ñ–Ω–Ω—è"""
        
        theories_data = self.data.get('aging_theories', {})
        
        # –¢–µ–æ—Ä—ñ—ó —Å—Ç–∞—Ä—ñ–Ω–Ω—è
        theories = []
        for theory_id, theory in theories_data.get('theories', {}).items():
            theories.append({
                'name': theory['name'],
                'mechanism': theory['mechanism'],
                'intervention': theory['intervention'],
                'supplements': theory.get('supplements', []),
                'emoji': theory.get('emoji', 'üî¨')
            })
        
        return {
            'type': 'theories',
            'title': 'üî¨ –ù–∞—É–∫–æ–≤—ñ —Ç–µ–æ—Ä—ñ—ó —Å—Ç–∞—Ä—ñ–Ω–Ω—è',
            'description': '–†–æ–∑—É–º—ñ–Ω–Ω—è –º–µ—Ö–∞–Ω—ñ–∑–º—ñ–≤ —Å—Ç–∞—Ä—ñ–Ω–Ω—è —Ç–∞ —Å–ø–æ—Å–æ–±—ñ–≤ –≤–ø–ª–∏–≤—É',
            'theories': theories,
            'key_insights': [
                'üî¨ –°—Ç–∞—Ä—ñ–Ω–Ω—è - –±–∞–≥–∞—Ç–æ—Ñ–∞–∫—Ç–æ—Ä–Ω–∏–π –ø—Ä–æ—Ü–µ—Å',
                'üéØ –†—ñ–∑–Ω—ñ –º–µ—Ö–∞–Ω—ñ–∑–º–∏ –ø–æ—Ç—Ä–µ–±—É—é—Ç—å —Ä—ñ–∑–Ω–∏—Ö –ø—ñ–¥—Ö–æ–¥—ñ–≤',
                'üíä –ö–æ–º–±—ñ–Ω–æ–≤–∞–Ω—ñ —ñ–Ω—Ç–µ—Ä–≤–µ–Ω—Ü—ñ—ó –µ—Ñ–µ–∫—Ç–∏–≤–Ω—ñ—à—ñ',
                'üîÑ –ü—Ä–æ—Ñ—ñ–ª–∞–∫—Ç–∏–∫–∞ –∫—Ä–∞—â–∞ –∑–∞ –ª—ñ–∫—É–≤–∞–Ω–Ω—è'
            ],
            'actionable_takeaways': [
                '–ê–Ω—Ç–∏–æ–∫—Å–∏–¥–∞–Ω—Ç–∏ –ø—Ä–æ—Ç–∏ –≤—ñ–ª—å–Ω–∏—Ö —Ä–∞–¥–∏–∫–∞–ª—ñ–≤',
                '–ì–æ–ª–æ–¥—É–≤–∞–Ω–Ω—è –¥–ª—è —Å—Ç–∏–º—É–ª—è—Ü—ñ—ó –∞—É—Ç–æ—Ñ–∞–≥—ñ—ó',
                '–ö–æ–Ω—Ç—Ä–æ–ª—å –≥–ª—é–∫–æ–∑–∏ –ø—Ä–æ—Ç–∏ –≥–ª—ñ–∫–∞—Ü—ñ—ó',
                'NAD+ –±—É—Å—Ç–µ—Ä–∏ –¥–ª—è –∫–ª—ñ—Ç–∏–Ω–Ω–æ—ó –µ–Ω–µ—Ä–≥—ñ—ó',
                '–ü—Ä–æ—Ç–∏–∑–∞–ø–∞–ª—å–Ω–∞ –¥—ñ—î—Ç–∞ –ø—Ä–æ—Ç–∏ inflammaging',
                '–°–µ–Ω–æ–ª—ñ—Ç–∏–∫–∏ –¥–ª—è –æ—á–∏—â–µ–Ω–Ω—è —Å—Ç–∞—Ä–∏—Ö –∫–ª—ñ—Ç–∏–Ω'
            ]
        }
    
    def handle_general_antiaging(self, query: str, user, context: Dict) -> Dict:
        """–ó–∞–≥–∞–ª—å–Ω–∞ —ñ–Ω—Ñ–æ—Ä–º–∞—Ü—ñ—è –ø—Ä–æ anti-aging"""
        
