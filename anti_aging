    def handle_cognitive(self, query: str, user, context: Dict) -> Dict:
        """–û–±—Ä–æ–±–∫–∞ –∑–∞–ø–∏—Ç—ñ–≤ –ø—Ä–æ –∫–æ–≥–Ω—ñ—Ç–∏–≤–Ω–µ –∑–¥–æ—Ä–æ–≤'—è"""
        
        cognitive_data = self.data.get('cognitive_enhancement', {})
        
        # –†–∞–Ω–∫–æ–≤–∏–π —Ñ–æ–∫—É—Å —Å—Ç–µ–∫
        morning_stack = []
        for supplement in cognitive_data.get('morning_focus_stack', {}).get('supplements', []):
            morning_stack.append({
                'name': supplement['name'],
                'dosage': supplement['dosage'],
                'timing': supplement.get('timing', '–≤—Ä–∞–Ω—Ü—ñ'),
                'effects': supplement['effects'],
                'price': supplement.get('price_range', ''),
                'mechanism': supplement.get('mechanism', ''),
                'synergy': supplement.get('synergy', '')
            })
        
        # –ù–µ–π—Ä–æ–≥–µ–Ω–µ–∑ —Ç–∞ –ø–∞–º'—è—Ç—å
        memory_stack = []
        for supplement in cognitive_data.get('neurogenesis_memory', {}).get('supplements', []):
            memory_stack.append({
                'name': supplement['name'],
                'full_name': supplement.get('full_name', ''),
                'dosage': supplement['dosage'],
                'effects': supplement['effects'],
                'duration': supplement.get('duration', '–ø–æ—Å—Ç—ñ–π–Ω–æ'),
                'price': supplement.get('price_range', ''),
                'mechanism': supplement.get('mechanism', '')
            })
        
        # –ü—Ä–æ—Ç–æ–∫–æ–ª–∏
