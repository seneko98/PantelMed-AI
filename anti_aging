    def handle_senolytics(self, query: str, user, context: Dict) -> Dict:
        """–û–±—Ä–æ–±–∫–∞ –∑–∞–ø–∏—Ç—ñ–≤ –ø—Ä–æ —Å–µ–Ω–æ–ª—ñ—Ç–∏–∫–∏ —Ç–∞ –ø–µ–ø—Ç–∏–¥–∏"""
        
        senolytics_data = self.data.get('senolytics_peptides', {})
        
        # –°–µ–Ω–æ–ª—ñ—Ç–∏–∫–∏
        senolytics = []
        for supplement in senolytics_data.get('senolytics', {}).get('supplements', []):
            senolytics.append({
                'name': supplement['name'],
                'dosage': supplement['dosage'],
                'frequency': supplement['frequency'],
                'mechanism': supplement['mechanism'],
                'effects': supplement['effects'],
                'price': supplement.get('price_range', ''),
                'evidence': supplement.get('evidence_level', 'medium'),
                'timing': supplement.get('timing', ''),
                'suitable_for': supplement.get('suitable_for', '')
            })
        
        # –ü–µ–ø—Ç–∏–¥–∏
        peptides = []
        for peptide in senolytics_data.get('anti_aging_peptides', {}).get('peptides', []):
            peptides.append({
                'name': peptide['name'],
                'mechanism': peptide['mechanism'],
                'dosage': peptide['dosage'],
                'frequency': peptide.get('frequency', ''),
                'effects': peptide['effects'],
                'cost': peptide.get('cost', ''),
                'evidence': peptide.get('evidence_level', 'experimental'),
                'requirements': peptide.get('requirements', ''),
                'suitable_for': peptide.get('suitable_for', '')
            })
        
        # –ü—Ä–∏—Ä–æ–¥–Ω—ñ —Å–µ–Ω–æ–ª—ñ—Ç–∏–∫–∏
        natural_senolytics = senolytics_data.get('natural_senolytics', {})
        
        # –ü—Ä–æ—Ç–æ–∫–æ–ª–∏
        protocols = senolytics_data.get('senolytics', {}).get('protocols', {})
        
        return {
            'type': 'senolytics',
            'title': 'üßü –°–µ–Ω–æ–ª—ñ—Ç–∏–∫–∏ —Ç–∞ –æ—á–∏—â–µ–Ω–Ω—è —Å—Ç–∞—Ä—ñ—é—á–∏—Ö –∫–ª—ñ—Ç–∏–Ω',
            'description': '–í–∏–¥–∞–ª–µ–Ω–Ω—è —Å–µ–Ω–µ—Å—Ü–µ–Ω—Ç–Ω–∏—Ö –∫–ª—ñ—Ç–∏–Ω –¥–ª—è –æ–º–æ–ª–æ–¥–∂–µ–Ω–Ω—è —Ç–∫–∞–Ω–∏–Ω',
            'senolytics': senolytics,
            'peptides': peptides,
            'natural_senolytics': natural_senolytics,
            'protocols': protocols,
            'key_benefits': [
                'üóëÔ∏è –í–∏–¥–∞–ª–µ–Ω–Ω—è "–∫–ª—ñ—Ç–∏–Ω-–∑–æ–º–±—ñ"',
                'üî• –ó–Ω–∏–∂–µ–Ω–Ω—è —Ö—Ä–æ–Ω—ñ—á–Ω–æ–≥–æ –∑–∞–ø–∞–ª–µ–Ω–Ω—è',
                'üîÑ –°—Ç–∏–º—É–ª—è—Ü—ñ—è —Ä–µ–≥–µ–Ω–µ—Ä–∞—Ü—ñ—ó —Ç–∫–∞–Ω–∏–Ω',
                '‚ö° –ü–æ–∫—Ä–∞—â–µ–Ω–Ω—è —Ñ—É–Ω–∫—Ü—ñ—ó –æ—Ä–≥–∞–Ω—ñ–≤'
            ],
            'implementation_strategy': [
